{
  "name": "@4s1/some-library",
  "version": "5.0.1",
  "description": "Some library",
  "bugs": {
    "url": "https://gitlab.com/4s1/playground/some-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/playground/some-library.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "npm run build -- --project tsconfig.dev.json",
    "start": "node dist/index.js",
    "format": "prettier --write src/",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "echo no linting",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "test": "echo no tests"
  },
  "prettier": "@4s1/eslint-config",
  "dependencies": {
    "@4s1/toolbox": "2.0.0"
  },
  "devDependencies": {
    "@4s1/eslint-config": "3.1.2",
    "@4s1/ts-config": "1.1.0",
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-conventional": "13.2.0",
    "@types/node": "14.17.20",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "prettier": "2.4.1",
    "standard-version": "9.3.1",
    "typescript": "4.4.3"
  },
  "publishConfig": {
    "@4s1:registry": "https://gitlab.com/api/v4/projects/28705297/packages/npm/"
  }
}
